{
  "name": "@appigram/react-jplayer",
  "version": "7.2.6",
  "description": "Html5 audio and video player library for React",
  "author": "Eugene Sysmanov <appigram@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/appigram/react-jPlayer.git"
  },
  "bugs": {
    "url": "https://github.com/appigram/react-jPlayer/issues"
  },
  "keywords": [
    "audio",
    "video",
    "video-player",
    "audio-player",
    "react",
    "redux",
    "jplayer",
    "javascript",
    "html5",
    "html5-audio",
    "html5-video",
    "media"
  ],
  "homepage": "https://github.com/appigram/react-jPlayer",
  "license": "MIT",
  "main": "./lib/index.js",
  "files": [
    "dist",
    "lib",
    "src"
  ],
  "scripts": {
    "lint": "eslint --fix --ext .jsx,.js ./src",
    "coverage": "cross-env BABEL_ENV=test nyc mocha src/**/*.spec.{js,jsx} --opts ./mocha.opts",
    "test": "cross-env BABEL_ENV=test mocha src/**/*.spec.{js,jsx} --opts ./mocha.opts",
    "test:watch": "npm test -- -w",
    "packages": "david",
    "packages:update": "david update",
    "dist": "rimraf dist && webpack",
    "build": "rimraf lib && babel src --out-dir lib --ignore spec.js,spec.jsx",
    "preversion": "npm test && npm run lint",
    "version": "npm run dist && npm run build && git add -A dist",
    "postversion": "git push && git push --tags",
    "toc": "markdown-toc -i README.md && git add README.md"
  },
  "pre-commit": [
    "lint",
    "test",
    "toc"
  ],
  "browserslist": [
    "chrome >= 15",
    "firefox >= 15",
    "explorer >= 9",
    "edge >= 12",
    "opera >= 16",
    "safari >= 5.1",
    "ios_saf >= 5.1",
    "chromeAndroid >= 36",
    "android >= 4",
    "ie_mob >= 9"
  ],
  "nyc": {
    "extension": [
      ".jsx"
    ],
    "include": "src",
    "exclude": [
      "**/*.spec.{js,jsx}"
    ],
    "require": [
      "@babel/register"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "all": true,
    "cache": true,
    "sourceMap": false,
    "instrument": false
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.5.5",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-istanbul": "^5.2.0",
    "babili-webpack-plugin": "^0.1.2",
    "coveralls": "^3.0.6",
    "cross-env": "^5.2.0",
    "css-loader": "^3.2.0",
    "david": "^11.0.0",
    "editorconfig": "^0.15.3",
    "enzyme": "^3.10.0",
    "eslint": "^6.2.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "expect": "^1.20.2",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "jsdom": "^15.1.1",
    "less": "^3.10.1",
    "less-loader": "^5.0.0",
    "markdown-toc": "^1.2.0",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "pre-commit": "^1.2.2",
    "proxyquire": "^2.1.3",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.0",
    "react-test-renderer": "^16.9.0",
    "redux": "^4.0.4",
    "redux-mock-store": "^1.5.3",
    "rimraf": "^3.0.0",
    "style-loader": "^1.0.0",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0"
  },
  "dependencies": {
    "@appigram/react-jplayer-utils": "^3.2.4",
    "classnames": "^2.2.6",
    "enzyme-adapter-react-16": "^1.14.0",
    "lodash.merge": "^4.6.2",
    "prop-types": "^15.7.2",
    "raf": "^3.4.1",
    "react": "^16.9.0",
    "react-motion": "^0.5.2",
    "react-transition-group": "^4.2.2",
    "recompose": "^0.30.0",
    "screenfull": "^4.2.1"
  }
}
